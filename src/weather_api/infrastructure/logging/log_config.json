{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "INFO"
    }
  },
  "formatters": {
    "default": {
      "format": "%(asctime)s [%(name)s] %(levelname)s %(message)s",
      "datefmt": "%d-%m-%y %H:%M:%S"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "console"
      ],
      "level": "INFO"
    }
  }
}
